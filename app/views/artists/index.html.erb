<h1>My my...all these artists...</h1>

<div class="container">
  <% @artists.each do |artist| %>
  <div class="ui card" >
    <div class="content">
      <h2><%= artist.name %></h2>
    </div>
    <div class="extra content">
      <button><%= link_to 'Show', artist_path(artist) %></button>
      <button><%= link_to 'Delete', artist, method: :delete %></button>
    </div>
  </div>
  <% end %>
</div>
