<h1>Find all your billboards here!</h1>
<hr>

<% @billboards.each do |billboard| %>
  <% if billboard.id != 6 %>
    <h2>Top <%= billboard.name %> Billboard</h2>
    <%= link_to 'Show', billboard_path(billboard) %>
    <%= link_to 'Edit', edit_billboard_path(billboard) %>
    <%= link_to 'Delete', billboard, method: :delete %>
  <% end %>
<% end %>

<hr>
<h3>Don't see the one you want?<%= link_to ' Add it!', new_billboard_path %><h3>
<h3>Want to see all the <%= link_to 'artists', artists_path %>?</h3>
<h3>Want to see all the <%= link_to 'songs', songs_path %>?</h3>
